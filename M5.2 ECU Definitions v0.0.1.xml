	<table type="3D" name="Ignition Timing - Part Load - Warm" category="Ignition Timing" storagetype="uint8" endian="little" sizex="12" sizey="16" userlevel="1" >
		<scaling units="&deg;BTDC" expression="(x*.75)-48" to_byte="(x+48)/0.75" format="0.00" fineincrement=".1" coarseincrement="1" />
			<table type="X Axis" name="Load" storagetype="uint8" endian="little" logparam="E2" >
				<scaling units="ms" expression="x*.05" to_byte="x/.05" format="0.00" fineincrement="1" coarseincrement="10" />
			</table>
			<table type="Y Axis" name="Engine Speed" storagetype="uint8" endian="little" logparam="P8">
				<scaling units="RPM" expression="x*40" to_byte="x/40" format="#" fineincrement="50" coarseincrement="100" />
			</table>
		<description>Zündwinkelkennfeld Motor warm. This is the ignition timing map when the engine has reached operating temp</description>
	</table>

