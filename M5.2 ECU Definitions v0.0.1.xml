	<table type="2D" name="Configuration Byte" category="Control Bits" storagetype="uint8" endian="little" sizey="1" userlevel="1">
		<scaling units="Decimal Value" expression="x" to_byte="x"  format="0" fineincrement="1" coarseincrement="10" />
			<table type="Static Y Axis" name="Convert from Decimal to Binary. Flip bits as needed. Then convert back to Decimal from Binary" sizey="1">
				<data>.</data>
			</table>
		<description>Unclear on use at this point in time</description>
	</table>
	

	<table type="BitwiseSwitch" name="Bit 0" category="Configurations">
		<bit position ="0" name="Enabled"/>
		<description>Bit 0 switch.</description>
	</table>

	<table type="BitwiseSwitch" name="Bit 1" category="Configurations">
		<bit position ="1" name="Enabled"/>
		<description>Bit 1 switch.</description>
	</table>

